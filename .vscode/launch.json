{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "nodemon",
      "runtimeArgs": ["--inspect=9229", "-r", "dotenv/config", "src/index.mjs", "--ignore", "history/"],
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}